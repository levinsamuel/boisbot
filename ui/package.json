{
  "name": "ui-react",
  "version": "0.1.0",
  "private": true,
  "dependencies": {
    "bootstrap": "^4.3.1",
    "event-stream": "3.3.4",
    "font-awesome": "^4.7.0",
    "react": "^16.6.3",
    "react-dom": "^16.6.3",
    "react-redux": "^5.1.1",
    "react-redux-form": "^1.16.10",
    "react-router-dom": "^4.3.1",
    "react-scripts": "5.0.1",
    "react-spinners": "^0.4.7",
    "reactstrap": "^6.5.0",
    "redux": "^4.0.1",
    "redux-logger": "^3.0.6",
    "redux-thunk": "^2.3.0"
  },
  "devDependencies": {
    "node-sass": "^4.13.1",
    "npm-run-all": "^4.1.3",
    "onchange": "^5.1.0"
  },
  "scripts": {
    "start": "run-p watch:scss react:start",
    "react:start": "react-scripts start",
    "scss": "node-sass -o \"src/styles/\" \"src/styles/\"",
    "watch:scss": "onchange \"src/styles/*.scss\" -- npm run scss",
    "build": "npm run scss && react-scripts build",
    "test": "react-scripts test",
    "eject": "react-scripts eject"
  },
  "eslintConfig": {
    "extends": "react-app"
  },
  "browserslist": [
    ">0.2%",
    "not dead",
    "not ie <= 11",
    "not op_mini all"
  ],
  "proxy": "http://localhost:5000"
}
